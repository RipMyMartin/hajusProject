{
    "openapi": "3.0.2",
    "info": {
        "title": "Games-API",
        "version": "1.0.0",
        "description": "A brand new API with no content.  Go nuts!"
    },
    "paths": {
        "/games": {
            "get": {
                "tags": [
                    "games"
                ],
                "responses": {
                    "200": {
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "type": "string"
                                    }
                                },
                                "examples": {
                                    "List of games": {
                                        "value": [
                                            "Witcher 3",
                                            "Cyberpunk 2077"
                                        ]
                                    }
                                }
                            }
                        },
                        "description": "application/json\n"
                    }
                },
                "summary": "Get a list of all games",
                "description": "returns a list of all games in API\n"
            }
        }
    }
}